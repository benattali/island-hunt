<% @island = Island.find(params[:id]) %>
<%= simple_form_for([@island, @booking]) do |f| %>
  <%= f.input :start_date, as: :string, input_html: {class: "datepicker", id:"start_date"} %>
  <%= f.input :end_date, as: :string, input_html: {class: "datepicker", id: "end_date"} %>
  <%= f.button :submit, "Book this island", class: "btn btn-primary btn-block" %>
<% end %>
